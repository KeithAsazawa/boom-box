<tr>
	<td><%= song.artist %></td>
	<td><%= song.title %></td>
	<td><%= song.description %></td>
	<td><%= song.genre %></td>
	<td><%= link_to 'Download', song %></td>
	<% if current_user == song.user %>
		<td><%= link_to 'Delete', song, method: :delete, data: { confirm: 'Are you sure?' } %></td>
	<% end %>
</tr>